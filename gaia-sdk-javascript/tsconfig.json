{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "inlineSources": true,
        "allowJs": false,
        "rootDir": "./src",
        "outDir": "./dist",
        "strict": true,
        "suppressImplicitAnyIndexErrors": true,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "es5",
            "es2015.promise",
            "dom",
            "es2015",
            "es6"
        ],
        "declaration": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "esModuleInterop": true
    },
    "files": [
        "./src/Gaia.ts"
    ],
    "compileOnSave": false,
    "buildOnSave": false
}
