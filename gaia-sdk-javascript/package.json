{
    "name": "@leftshiftone/gaia-sdk",
    "version": "3.4.0-rc.3",
    "description": "JavaScript SDK for the GAIA ecosystem.",
    "main": "dist/all.js",
    "dependencies": {
        "axios": "^0.19.2",
        "bytebuffer": "^5.0.1",
        "cross-blob": "^2.0.0",
        "crypto-js": "^3.1.9-1",
        "form-data": "^3.0.0",
        "mqtt": "^4.1.0",
        "rxjs": "^6.5.5",
        "uuid": "^7.0.3",
        "validator": "^13.0.0"
    },
    "devDependencies": {
        "@types/crypto-js": "^3.1.43",
        "@types/jest": "^26.0.15",
        "@types/mqtt": "^2.5.0",
        "@types/node": "^10.7.1",
        "@types/sinon": "^7.0.5",
        "@types/uuid": "^7.0.3",
        "html-webpack-plugin": "^3.2.0",
        "import-glob-loader": "^1.1.0",
        "jest": "^26.6.0",
        "jest-junit": "5.1.0",
        "release-it": "^10.3.1",
        "shx": "^0.3.2",
        "sinon": "^7.2.3",
        "sonarqube-scanner": "2.1.2",
        "style-loader": "^0.22.1",
        "stylelint": "^9.5.0",
        "stylelint-config-recommended": "^2.1.0",
        "stylelint-webpack-plugin": "^0.10.5",
        "ts-jest": "^26.4.1",
        "ts-loader": "^8.0.6",
        "tslint": "^6.1.3",
        "tslint-config-airbnb": "^5.11.2",
        "tslint-loader": "^3.5.4",
        "typescript": "^4.0.3",
        "uglifyjs-webpack-plugin": "^1.3.0",
        "webpack": "^4.16.5",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.11"
    },
    "jest-junit": {
        "suiteName": "Frontend Tests",
        "suiteNameTemplate": "{filename}",
        "output": "build/test/TEST-junit.xml",
        "classNameTemplate": "{classname}-{title}",
        "titleTemplate": "{classname}-{title}"
    },
    "scripts": {
        "webpack:dev": "webpack-dev-server --open --mode=development",
        "webpack:build": "webpack --mode=production",
        "clean": "shx rm -rf dist",
        "build": "tsc && tsc -m es2015 --outDir ./dist/esm2015 && npm run webpack:build",
        "test": "jest",
        "test-node": "jest --env=node",
        "coverage": "jest --coverage",
        "lint": "stylelint tslint src/**/*.ts",
        "sonarqube": "jest --coverage && sonar-scanner -Dsonar.host.url=$SONAR_HOST_URL -Dsonar.login=$SONAR_LOGIN"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leftshiftone/gaia-sdk.git"
    },
    "keywords": [
        "sdk",
        "gaia",
        "leftshiftone"
    ],
    "author": "Leftshift One Software GmbH <contact@leftshift.one>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/leftshiftone/gaia-sdk/issues"
    },
    "homepage": "https://github.com/leftshiftone/gaia-sdk",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    }
}