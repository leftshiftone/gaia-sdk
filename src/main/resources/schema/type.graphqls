# ****************
# Experience types
# ****************

type BehaviourExecution {
    processInstanceId: Uuid
    state: String
    timestamp: ISO8601
    duration: ISO8601
    startEventType: String
    startEventId: Uuid
    initAttributes: Struct
    processId: Uuid
    parentProcessId: Uuid
}
type BehaviourNodeExecution {
    processInstanceId: Uuid
    nodeInstanceId: Uuid
    state: String
    executionGroupId: Uuid
    nodeId: Uuid
    processId: Uuid
    type: String
    transitions: Struct
    timestamp: ISO8601
    parentProcessId: Uuid
}
