type Introspection {
    cpu: String
    gpu: String
    memory: String
    state: RuntimeState
    started: Timestamp
    skills: [SkillIntrospection]
}
type SkillIntrospection {
    name: String
    state: SkillState
    started: Timestamp
}

enum SkillState {
    RUNNING
}
enum RuntimeState {
    RUNNING
}
