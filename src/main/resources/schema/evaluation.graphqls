type Evaluation {
    skill: SkillEvaluation
    buildIn: BuildInEvaluation
}

type BuildInEvaluation {
    behaviour: String
    gaiaquery: String
}

type SkillEvaluation {
    syncEval(impulse: String): SyncSkillEvaluation
    asyncEval(impulse: String): AsyncSkillEvaluation
}

type SyncSkillEvaluation {
    tbd: String
}

type AsyncSkillEvaluation {
    tbd: String
}
